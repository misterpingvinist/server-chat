{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "strict": true,
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "target": "es5",
    "experimentalDecorators": true,
    "interface-name": [true, "never-prefix"]
  },
  "include": ["./src/**/*"]
}
